<p id="notice"><%= notice %></p>

<h1>Listing Custom Field Values</h1>

<table>
  <thead>
    <tr>
      <th>Contact</th>
      <th>Custom field</th>
      <th>Value</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @custom_field_values.each do |custom_field_value| %>
      <tr>
        <td><%= custom_field_value.contact %></td>
        <td><%= custom_field_value.custom_field %></td>
        <td><%= custom_field_value.value %></td>
        <td><%= link_to 'Show', custom_field_value %></td>
        <td><%= link_to 'Edit', edit_custom_field_value_path(custom_field_value) %></td>
        <td><%= link_to 'Destroy', custom_field_value, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Custom field value', new_custom_field_value_path %>
